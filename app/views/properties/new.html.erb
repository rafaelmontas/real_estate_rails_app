<%= form_for @property, html: { multipart: true } do |f| %>

  <%= f.file_field :image %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :price %>
  <%= f.number_field :price %>

  <%= f.submit "Publicar", class: "btn btn-primary" %>
<% end %>
